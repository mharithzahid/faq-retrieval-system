generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Faq {
  id        Int      @id @default(autoincrement())
  question  String
  answer    String
  tags      String[]
  lang      String   @default("en")

  embedding Unsupported("vector")?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}